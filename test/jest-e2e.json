{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^generated/(.*)$": "<rootDir>/../generated/$1",
    "^src/database/(.*)$": "<rootDir>/../src/database/$1",
    "^src/interfaces/(.*)$": "<rootDir>/../src/interfaces/$1",
    "^src/auth/decorators/(.*)$": "<rootDir>/../src/auth/decorators/$1"
  }
}